<odoo>
    <record id="view_document_report_wizard_form" model="ir.ui.view">
        <field name="name">document.report.wizard.form</field>
        <field name="model">document.report.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="date_from"/>
                    <field name="date_to"/>
                </group>
                <footer>
                    <button string="Generuoti ataskaitą" type="object" class="oe_highlight" icon="fa-check" name="button_generate_report"/>
                    <button string="Atšaukti" class="oe_secondary" icon="fa-times" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

